    phrase 1</mark></link>\n\n\u041D\u0435 \u043C\u043E\u0433\u0443 \u0434\u043E\u0436\u0434\u0430\u0442\u044C\u0441\u044F
    \u043D\u0430\u0448\u0435\u0439 \u0442\u0430\u0439\u043D\u043E\u0439 \u0432\u0441\u0442\u0440\u0435\u0447\u0438\u2026
    \u0417\u043D\u0430\u044E, \u0442\u044B \u0441\u0447\u0438\u0442\u0430\u0435\u0448\u044C
    \u043C\u0435\u043D\u044F \u0438\u0437\u043B\u0438\u0448\u043D\u0435 \u0447\u0443\u0432\u0441\u0442\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0439
    \u0438 \u0441\u0435\u043D\u0442\u0438\u043C\u0435\u043D\u0442\u0430\u043B\u044C\u043D\u043E\u0439,
    \u043D\u043E \u043A\u0430\u043A \u0442\u043E\u043B\u044C\u043A\u043E \u044F \u043F\u043E\u0434\u0443\u043C\u0430\u044E
