    \u044D\u0442\u0443 \u043D\u043E\u0447\u044C \u0432\u0434\u0432\u043E\u0435\u043C,
    \u0442\u043E\u043B\u044C\u043A\u043E \u0442\u044B \u0438 \u044F\u2026 \u042F \u0443\u0436\u0435
    \u0437\u0430\u043D\u044F\u043B\u0430 \u043D\u0430\u043C \u043A\u043E\u043C\u043D\u0430\u0442\u0443
    \u0432 \u0433\u043E\u0441\u0442\u0438\u043D\u0438\u0446\u0435, \u0436\u0434\u0443
    \u0442\u0435\u0431\u044F \u0437\u0434\u0435\u0441\u044C. \u0422\u044B \u043D\u0435
    \u043F\u043E\u0436\u0430\u043B\u0435\u0435\u0448\u044C \u0447\u0442\u043E \u043F\u0440\u0438\u0435\u0445\u0430\u043B,
